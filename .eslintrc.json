{
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6,
    "impliedStrict": true,
    "sourceType": "module"
  },
  "env": {
    "node": true,
    "es6": true
  },
  "extends": "eslint:recommended",
  "rules": {
    "no-console": 0,
    "keyword-spacing": 2
  },
  "plugins": ["flowtype"]
}